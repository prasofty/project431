<!doctype html>
<html ng-app="project431">
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Project431" %></title>

    <%= stylesheet_link_tag "bootstrap.min" %>
    <%= stylesheet_link_tag "app" %>
  </head>

  <body class="<%= page_classes %>">
    <%= yield %>
    <%= javascript_include_tag "jquery.min" %>
    <%= javascript_include_tag "bootstrap.min" %>
    <!-- Angular JS files -->
    <%= javascript_include_tag "angular.min" %>
    <%= javascript_include_tag "angular-ui-router" %>
    <%= javascript_include_tag "ui-bootstrap-tpls-0.14.2" %>

    <!-- Parse JS SDK -->
    <%= javascript_include_tag "parse-1.6.7" %>
    <%= javascript_include_tag "app" %>
    <%= javascript_include_tag "controllers/header_controller" %>
    <%= javascript_include_tag "controllers/session_controller" %>
  </body>
</html>
